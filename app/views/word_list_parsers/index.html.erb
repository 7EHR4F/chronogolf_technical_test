<p id="notice"><%= notice %></p>

<h1>Word List Parsers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_list_parsers.each do |word_list_parser| %>
      <tr>
	<td><%= word_list_parser.name %></td>
	<td><%= word_list_parser.result %></td>
        <td><%= link_to 'Show', word_list_parser %></td>
        <td><%= link_to 'Edit', edit_word_list_parser_path(word_list_parser) %></td>
        <td><%= link_to 'Destroy', word_list_parser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word List Parser', new_word_list_parser_path %>
